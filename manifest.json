{
  "manifest_version": 2,
  "name": "Navver",
  "description": "Simple, lightweight mouseless navigation",
  "version": "0.4",
  "icons": {
    "128": "logo-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "element.js",
        "navver-element.js",
        "label-generator.js",
        "navigator-input.js",
        "navigator.js",
        "scroll-up.js",
        "scroll-down.js",
        "scroll-left.js",
        "scroll-right.js",
        "scroll-to-top.js",
        "scroll-to-bottom.js",
        "application.js"
      ]
    }
  ]
}
