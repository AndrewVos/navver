{
  "manifest_version": 2,

  "name": "Navver",
  "description": "Simple, lightweight mouseless navigation",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "reset.js",
        "element-finder.js",
        "navver.js",
        "search-navigator.js",
        "keyboard-navigator.js",
        "scroll-down-shortcut.js",
        "scroll-up-shortcut.js",
        "scroll-to-top-shortcut.js",
        "scroll-to-bottom-shortcut.js"
      ]
    }
  ]
}
