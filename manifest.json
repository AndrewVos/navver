{
  "manifest_version": 2,
  "name": "Navver",
  "description": "Simple, lightweight mouseless navigation",
  "version": "0.9",
  "icons": {
    "128": "logo-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "element.js",
        "label-generator.js",
        "application.js"
      ]
    }
  ]
}
