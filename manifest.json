{
  "manifest_version": 2,

  "name": "Navver",
  "description": "Simple, lightweight mouseless navigation",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "reset.js",
        "element-finder.js",
        "navver.js",
        "search-navigator.js",
        "keyboard-navigator.js",
        "scroll-shortcut.js"
      ]
    }
  ]
}
